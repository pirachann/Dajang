<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/dajang"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dajang"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dajang"
        tools:targetApi="31">
        <activity
            android:name=".ui.game.quiz.LevelQuizActivity"
            android:exported="false"
            android:parentActivityName=".ui.MenuActivity"/>
        <activity
            android:name=".ui.MenuActivity"
            android:exported="false"
            android:theme="@style/Fullscreen" />
        <activity
            android:name=".ui.game.QuizActivity1"
            android:exported="false"
            android:parentActivityName=".ui.MenuActivity"
            android:theme="@style/Fullscreen"/>
        <activity
            android:name=".ui.game.QuizActivity2"
            android:exported="false"
            android:parentActivityName=".ui.MenuActivity"
            android:theme="@style/Fullscreen"/>
        <activity
            android:name=".ui.AboutActivity"
            android:exported="false"
            android:parentActivityName=".ui.HomeActivity" />
        <activity
            android:name=".ui.ListActivity"
            android:exported="false"
            android:parentActivityName=".ui.HomeActivity" />
        <activity
            android:name=".ui.HomeActivity"
            android:exported="false"
            android:parentActivityName=".ui.MenuActivity" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="true"
            android:theme="@style/Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>